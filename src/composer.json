{
    "name": "rialic/starter-kit",
    "description": "Laravel wrapper packge with Repository Pattern, Service Layer Pattern and custom log configuration",
    "license": "MIT",
    "authors": [
        {
            "name": "Rhooiali CÃ¢ndido",
            "email": "rhiali_cs@hotmail.com"
        }
    ],
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": {
        "starter-kit" : {
            "type": "path",
            "url": "../../packages/rialic/starter-kit",
            "options": {
                "symlink": true
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Rialic\\StarterKit\\": "src/"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Rialic\\StarterKit\\Providers\\StarterKitServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "^11.36",
        "php": "^8.2",
        "rialic/starter-kit": "@dev"
    }
}
